{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*", "/css/*", "/js/*", "/*.ico", "/*.png", "/*.svg", "/*.jpg", "/*.webp"]
    },
    "routes": [
      {
        "route": "/*",
        "statusCode": 200
      }
    ],
    "mimeTypes": {
      ".json": "text/json",
      ".css": "text/css",
      ".js": "text/javascript",
      ".png": "image/png",
      ".jpg": "image/jpeg",
      ".webp": "image/webp"
    }
  }